{
    "name": "settermjd/php-weather-app",
    "description": "This is a small PHP web-based application that retrieves and renders the weather for a given city or town around the world.",
    "license": [
        "GPL-3.0-or-later"
    ],
    "require": {
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "slim/twig-view": "^3.3",
        "php-di/slim-bridge": "^3.4",
        "cmfcmf/openweathermap-php-api": "^3.3",
        "guzzlehttp/psr7": "^2.6",
        "guzzlehttp/guzzle": "^7.8",
        "vlucas/phpdotenv": "^5.6",
        "slim/flash": "^0.4.0",
        "monolog/monolog": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Weather\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeatherTest\\": "test/"
        }
    }
}
